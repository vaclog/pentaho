    <connection>VKM Prod</connection>
    <sql>
select
S1.SEntDocNro as 'Armado', EntID as 'Entidad', EntEntIdC as 'Cliente.Destino', EntNombre as 'Nombre', SEntFecAlt as 'FechaAlta',
S.SentFecFin as 'FechaFin', S.SentNroDsp as 'Documento', S.SentPCCId as 'PCC', S.LogEntId as 'ID.Cliente', S.SEntEstEnt as 'Estado',
S1.SEntArtId as 'Artid', ArtEtaId1 as 'Artículo', ArtNom as 'Descripción', SEntDUNCnt as 'Master/UxB', SEntIteCnt as 'Cnt.documento(Ud.)',
SEntBulCnt as 'Cnt.documento(Bul.)', SEntIteCon as 'Cnt.Preparada(Ud.)', SEntBulCnf as 'Cnt.Preparada(Bul.)'
from SENT1 S1 (nolock)
inner join SENT S (nolock) on S1.SEntDocNro = S.SEntDocNro
inner join ART (nolock) on SEntArtId = ARTID
left join ENT (nolock) on SEntEntId = EntID
where SentTVar = 'S' and SentTipUso = 'S'
and SEntFecAlt >= '2021-05-01'

and S.SentEstEnt IN ('CAR' , 'PRE')

order by SEntFecAlt asc</sql>
