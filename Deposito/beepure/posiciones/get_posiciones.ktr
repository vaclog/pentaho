    <sql>SELECT  DepUbiPrv                   AS 'cliente_id'
       ,'Muelle entrada de Insumos' AS 'ubicacion'
       ,DepPosFecI                  AS 'fecha_ingreso'
       ,DepPosC                     AS 'posicion'
       ,DepPosPick                  AS 'picking'
       ,DepPalId                    AS 'pallet'
       ,ArtEtaId1                   AS 'sku'
       ,DepCntDisF                  AS 'cantidad'
       ,CAST( GETDATE() AS Date )   AS 'fecha_proceso'
       ,C.CatNom
FROM DEPUBI
LEFT JOIN art
ON DepArtId = ARTID
LEFT JOIN ART2 A2
ON art.ARTID = A2.ARTID
LEFT JOIN CAT C
ON C.CatID = A2.CatID
WHERE depid IN ( SELECT DEPID FROM DEPSEC)
AND DepCntDisF > 0
AND DepSecCod IN ( SELECT DepSecCod FROM DEPSEC WHERE DepSecTipo = 'E')
AND DepUbiPrv = 13781
AND C.CatNom IS NOT NULL
UNION
SELECT  DepUbiPrv                           AS 'cliente_id'
       ,'Muelle entrada de Prod. Terminado' AS 'ubicacion'
       ,DepPosFecI                          AS 'fecha_ingreso'
       ,DepPosC                             AS 'posicion'
       ,DepPosPick                          AS 'picking'
       ,DepPalId                            AS 'pallet'
       ,ArtEtaId1                           AS 'sku'
       ,DepCntDisF                          AS 'cantidad'
       ,CAST( GETDATE() AS Date )           AS 'fecha_proceso'
       ,C.CatNom
FROM DEPUBI
LEFT JOIN art
ON DepArtId = ARTID
LEFT JOIN ART2 A2
ON art.ARTID = A2.ARTID
LEFT JOIN CAT C
ON C.CatID = A2.CatID
WHERE depid IN ( SELECT DEPID FROM DEPSEC)
AND DepCntDisF > 0
AND DepSecCod IN ( SELECT DepSecCod FROM DEPSEC WHERE DepSecTipo = 'E')
