    <connection>VOTS 4PL</connection>
    <sql>  
  select concat(ai.tripli, ai.artic, '') as clave, ai.tripli as articulo_operacion, 
		ai.canti as  articulo_cantidad, 
		ai.artic as articulo_codigo, 
		sum(ai.fobaju) as articulo_fob , 
        sum(ai.ecflete* ai.canti) as articulo_flete,
        sum(ai.ecseguro * ai.canti) as articulo_seguro
  from artici ai
  where ai.tripli IN (

	SELECT oi.tripli
      FROM operai oi
     
		
/*	where oi.feccar >=  str_to_date(${fecha_desde}, '%Y-%m-%d') 
	and oi.feccar &lt;= str_to_date(${fecha_hasta}, '%Y-%m-%d') 
	and oi.idcliente = ${cliente_id}
*/
	where oi.feccar >=  str_to_date('2019-08-01', '%Y-%m-%d') 
	and oi.feccar &lt;= str_to_date('2019-08-31', '%Y-%m-%d') 
	and oi.idcliente = 1521


		)
and ai.canti > 0
  group by ai.tripli, ai.artic

order by ai.artic
  </sql>
