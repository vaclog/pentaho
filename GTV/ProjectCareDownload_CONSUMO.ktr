    </partitioning>
    <connection>VOTS 4PL</connection>
    <sql>select calculo.clave, 
	sum(calculo.DetalleGastosxFacturaxOperacionxArticulo4pl_importe) as suma_articulo_gasto_local, 
	sum(calculo.DetalleGastosxFacturaxOperacionxArticulo4pl_importe_dolares) as 'Valor de impuesto pago para esta línea de partida arancelaria'
from (select 
concat(o.tripli, arti.artic) as clave,
a.idfactura_gasto,a.idgasto,a.id_artici,
 case when f.idmoneda='PES' THEN a.importe ELSE a.importe*o.tipcamofi end as DetalleGastosxFacturaxOperacionxArticulo4pl_importe,
 case when f.idmoneda='DOL' then a.importe else a.importe/o.tipcamofi end as DetalleGastosxFacturaxOperacionxArticulo4pl_importe_dolares
from 4pl_factura_gasto_articulo a
inner join 4pl_factura_gasto g on g.id=a.idfactura_gasto
inner join 4pl_factura f on f.id=g.idfactura
inner join artici arti on arti.id_artici=a.id_artici
inner join operai o on o.tripli=arti.tripli
	where o.feccar >=  str_to_date('2019-08-01', '%Y-%m-%d') 
	and o.feccar &lt;= str_to_date('2019-08-31', '%Y-%m-%d') 
	and o.idcliente = 1521
	and a.idgasto IN (2,3)
order by clave) as calculo

group by calculo.clave
order by calculo.clave</sql>
