  <step>
    <name>Query General</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>VOTS 4PL</connection>
    <sql>select distinct oi.tripli as tripli, 

concat(oi.tripli, ai.artic,'') as clave
, 'RUGGERO VACCARI Y ASOC SA' as 'Nombre del Agente de Aduana responsable por el reporte'
, 
concat(cl.cliente) as 'Nombre del importador'
, concat(cl.cuit) as 'Identificación del importador'
, 'AR' as 'Código del país'
, concat(pr.nombre, '') as 'Nombre del vendedor / exportador'
, concat(pais_proc.nombre,'') as 'Embarcado del país'
, '' as 'Número de guía B/L o AWB#'
, concat(oi.doccar,'') as 'Número de house B/L o AWB#'
, if( via.nombre = 'CAMION', 'TERRESTRE', if(via.nombre = 'AVION', 'AEREO', via.nombre)) as 'Mode de embarque'
, if(oi.feceent &lt;&gt;0, date_format(oi.feceent, '%d/%m/%Y'), null) as 'Fecha de llegada'
, if(oa.feccierredeposito &lt;&gt; 0, date_format(oa.feccierredeposito, '%d/%m/%Y'), null) as 'Fecha de entrada aduana'
, concat(oa.nrodespacho) as 'Número de referencia en la aduana'
, if(ai.feccar &lt;&gt; 0, date_format(oi.feccar, '%d/%m/%Y'), null) as 'Fecha liberación en la Aduana'
,null as 'Fecha de liberación en otros Organos de Gobierno'
, replace(ai.pos_arancelaria, '.', '') as 'Partida Arancelaria utilizada en la importación'
, concat(mo.ISO4217code) as 'Moneda'
, null as 'Otras tajas (IVA, VAT, o otros cobrados em el país)'
, concat(pais_origen.nombre, '') as 'País de fabricación de los productos'
, if( pais_origen.nombre = 'BRASIL'
		or pais_origen.nombre = 'URUGUAY'
		or pais_origen.nombre = 'PARAGUAY'
		or pais_origen.nombre = 'BOLIVIA'
		or pais_origen.nombre = 'VENEZUELA', 'Participa del Convenio Mercosur',
		if( pais_origen.nombre = 'COLOMBIA', 'Participa convenio lista 359/1', '')
		
		 ) as 'Participa de algun programa de Libre Comercio? Caso positivo indicar cual tratado y que SKU tienen benefício'
, concat(ai.artic) as 'SKU'


, concat(pais_origen.ISO3166code2char) as 'CountryOfOrigin'
, concat(pais_proc.ISO3166code2char) as 'ExportCountry'
  from operai oi
  join operaiadd oa on oa.numop = oi.tripli
  join clientes cl on cl.idcliente = oi.idcliente
  join proveedor pr on pr.id = oa.id_proveedor
  join paises pais_proc on pais_proc.idpais = oi.proced
  
  left join via on via.codvia = oi.codvia
  join artici ai on ai.tripli = oi.tripli and ai.canti > 0
	left join paises pais_origen on pais_origen.idpais = ai.origen2
  left join monedas mo on mo.cdev = oi.codfob
 


	where oi.feccar >=  str_to_date('2019-08-01', '%Y-%m-%d') 
	and oi.feccar &lt;= str_to_date('2019-08-31', '%Y-%m-%d') 
	and cl.idcliente = 1521




order by ai.tripli, ai.artic


/*	where oi.feccar >=  str_to_date(${fecha_desde}, '%Y-%m-%d') 
	and oi.feccar &lt;= str_to_date(${fecha_hasta}, '%Y-%m-%d') 
	and cl.idcliente = ${cliente_id}
*/
 </sql>
