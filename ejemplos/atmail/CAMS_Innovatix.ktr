    <connection>camsp</connection>
    <sql>SELECT to_char(cont.cont_num) cont_num,
       cont_internal_id, decode (note_txt, NULL, cont_title, &apos; &apos;, cont_title, note_txt) cont_title,
       whome.bunit_name, whome.BUNIT_ID_PRI,
       to_char(cont_dt_start, &apos;mm&#47;dd&#47;yyyy&apos;) cont_dt_start,
       to_char(cont_dt_end, &apos;mm&#47;dd&#47;yyyy&apos;) cont_dt_end,
       SUBSTR(lob.bunit_name,9) LOB, A.txt_tbc_val, contgrp_desc, ttbc.TXT_TBC_VAL, CHARITM_DESC,
       DECODE( MIN(cppt_percent), MAX(cppt_percent), MAX(cppt_percent)*100||&apos;%&apos;,
         MIN(cppt_percent)*100 || &apos;% - &apos; ||MAX(cppt_percent)*100 ||&apos;%&apos;) cppt_percent
  FROM cont, cpgrp, cppt, bunit whome, ttbc, ttbc a, status, contstat, bunit lob, note, charitm, contchar, whoami,
       contgrp, contgrpcont
 WHERE cpgrp.PGRPTYP_CD = &apos;PFD&apos; and ttbc.num_tbh_id = 501 and contstat.status_num in (6,36) and cppt.status_num in (60,61)
   and cppt_percent IS NOT NULL and ttbc.txt_tbc_cde = cont.CTAWDTYP_CD and cont.whoami_num = whoami.whoami_num
   and lob.bunit_num = whoami.bunit_num and cont.contstat_num = contstat.contstat_num
   and contstat.status_num = status.status_num and cpgrp.cont_num = cont.cont_num
   and cppt.cpgrp_num = cpgrp.cpgrp_num and contchar.cont_num(+) = cont.cont_num
   and contchar.charitm_num = charitm.charitm_num
   and whome.bunit_num = cont.ctorg_bunit_num and note_rownum(+) = cont.cont_num and notetyp_id(+) = &apos;CONTTITLE&apos;
   and cont.cont_dt_end &gt;= sysdate and cont.cont_num = contgrpcont.cont_num(+)
   and contgrpcont.contgrp_num  = contgrp.contgrp_num(+)
   and charset_id = &apos;COMMITMENT&apos; AND a.num_tbh_id = 517 AND a.txt_tbc_cde = cont.mkttyp_id
 Group by cont.cont_num, cont_internal_id, note_txt, cont_title, whome.bunit_name, whome.BUNIT_ID_PRI, cont_dt_start,
       cont_dt_end, SUBSTR(lob.bunit_name,9), a.txt_tbc_val, CONTgrp_DESC, ttbc.TXT_TBC_VAL, CHARITM_DESC            
 ORDER BY cont_internal_id</sql>
